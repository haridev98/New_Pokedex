<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hd.New_Pokedex">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hd.New_Pokedex.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hd.New_Pokedex.list"
            android:label="list"
            android:parentActivityName="com.hd.New_Pokedex.MainActivity" />
        <activity android:name="com.hd.New_Pokedex.pic"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
    </application>


</manifest>